/* pages/like.wxss */
.container {
  padding: 20rpx;
}
.nosong{
width: 60%;
height: 200rpx;
line-height:200rpx;
margin:0 auto;
text-align: center;
font-size: 70rpx;
}
.likeSongs {
  padding: 20rpx;
  background-color: white;
  margin: 20rpx;
  height: 100rpx;
  border: 1px solid gray;
  border-radius: 20rpx;
}
.likeSongs image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 10rpx;
  float:left;
}
.likeSongs view {
  height: 100rpx;
  line-height: 100rpx;
  float: left;
  text-align: center;
}
.songName {
  width: calc((100% - 100rpx) / 3 * 2);
}
.songAuthor {
  width: calc((100% - 100rpx) / 3);
}
